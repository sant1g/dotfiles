general {
        colors = false
        interval = 5
}

order += "disk /"
order += "ethernet enp4s0"
order += "load"
order += "tztime local"

ethernet enp4s0 {
        # if you use %speed, i3status requires the cap_net_admin capability
        format_up = "%ip"
        format_down = "E: down"
}

tztime local {
        format = "%d/%m/%Y %H:%M"
}

load {
        format = "%5min"
}

